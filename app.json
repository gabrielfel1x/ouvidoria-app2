{
  "expo": {
    "name": "Ouvidoria App",
    "slug": "ouvidoria-app2",
    "version": "1.0.0",
    "description": "Aplicativo de Ouvidoria para denúncias, reclamações, sugestões e elogios",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ouvidoriaapp2",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gabrielfelxx.ouvidoriaapp2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gabrielfelxx.ouvidoriaapp2",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Este app precisa acessar sua localização para identificar problemas na sua região."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fb9ad45a-6878-4b86-8d34-bdbc00e7b60a"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/fb9ad45a-6878-4b86-8d34-bdbc00e7b60a"
    }
  }
}
